{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run docs:build && vite",
    "dev:all": "concurrently \"npm run docs:build && vite\" \"vitepress dev docs\"",
    "build": "vite build && npm run docs:build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "update-api": "openapi-typescript http://localhost:8000/openapi.json -o src/api/schema.ts"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "dexie": "^4.2.1",
    "pdfjs-dist": "^5.4.530",
    "vue": "^3.5.24",
    "@tailwindcss/vite": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/axios": "^0.9.36",
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "openapi-typescript": "^7.10.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-checker": "^0.12.0",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.2.2"
  }
}